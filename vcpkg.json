{
    "name": "plasma",
    "version-string": "2.0.70.2",
    "description": "Cyan Worlds' Plasma game engine",
    "dependencies": [
      "asio",
      "curl",
      "expat",
      {
        "name": "fontconfig",
        "platform": "!(osx | windows | ios)"
      },
      "freetype",
      {
        "name": "libarchive",
        "platform": "osx | ios"
      },
      {
        "name": "libepoxy",
        "platform": "!(ios)"
      },
      "libjpeg-turbo",
      "libogg",
      "libpng",
      "libvorbis",
      "libvpx",
      "libwebm",
      {
        "name": "openal-soft",
        "features": [
          {
            "name": "pipewire",
            "platform": "linux"
          }
        ]
      },
      "openssl",
      "opus",
      "physx",
      {
        "name": "python3",
        "features": [
          "deprecated-win7-support"
        ],
        "platform": "!(ios)"
      },
      "speex",
      "string-theory",
      {
        "name": "libuuid",
        "platform": "linux"
      },
      "zlib"
    ],
    "overrides": [
      {
        "name": "physx",
        "version": "4.1.2#6",
        "$comment": "Upstream vcpkg updated to PhysX 5, which drops support for several target platforms. Stick with 4.1.2 for now."
      }
    ],
    "features": {
      "cairosvg": {
        "description": "Installs a functional cairosvg Python module for the resource.dat generator.",
        "dependencies": [
          {
            "name": "python3-cairosvg",
            "host": true,
            "platform": "!(windows & arm64) | !(ios)"
          }
        ]
      }
    },
    "builtin-baseline": "dd3097e305afa53f7b4312371f62058d2e665320",
    "vcpkg-configuration": {
      "overlay-ports": ["./Scripts/Ports"],
      "overlay-triplets": ["./Scripts/Triplets"]
    }
}
